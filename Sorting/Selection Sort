def Selection_Sort(arr):
    for i in range(len(arr)):
        minimum = i
        for j in range(i+1,len(arr)):
            if arr[j]<arr[minimum]:
                minimum = j
            
        arr[i], arr[minimum] = arr[minimum], arr[i]


#Output#

arr = [14,43,76,10,99,45]
print("Array is:",arr )
Selection_Sort(arr)
print("Sorted array is:",arr)
