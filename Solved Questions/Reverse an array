def Reverse_an_array(arr):

    for i in range(len(arr)//2):
        temp = arr[i]
        arr[i] = arr[len(arr) - i -1]
        arr[len(arr) - i - 1] = temp

    return arr

arr = list(map(int, input('Enter Elements of Array\n').split()))
print(Reverse_an_array(arr))

#Input#
Enter Elements of Array
1 3 5 7 9

#Output#
[9, 7, 5, 3, 1]
