def fibb(n):
    a = 0
    b = 1
    if n<=0:
        print("Cannot create a series")
    elif n==1:
        print('series is 0')
    else:
        for i in range(n):
            print(a, end=' ')
            temp = a + b
            a = b
            b = temp
        print()

fibb(7)
fibb(1)
fibb(-4)


#Output#

0 1 1 2 3 5 8 
series is 0
Cannot create a series
